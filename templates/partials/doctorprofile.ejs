<div class="offcanvas offcanvas-start" data-bs-scroll="true" data-bs-backdrop="false" tabindex="-1"
    id="offcanvasScrolling" aria-labelledby="offcanvasScrollingLabel">
    <div class="offcanvas-header">
      <h1 class="offcanvas-title" id="offcanvasScrollingLabel">Doctor Profile</h1>
      <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
    </div>
    <div class="offcanvas-body">
      <p><strong>Name : </strong> <span><%= docname %></span></p><br>
      <p><strong>Email : </strong><span><%= docemail %></span></p><br>
      <p><strong>Speciality: </strong><span><%= speciality %></span></p><br>
      <p><strong>Experience : </strong><span><%= experience %></span></p><br>



    </div>
  </div>