<div class="offcanvas offcanvas-start" data-bs-scroll="true" data-bs-backdrop="false" tabindex="-1"
    id="offcanvasScrolling" aria-labelledby="offcanvasScrollingLabel">
    <div class="offcanvas-header">
      <h1 class="offcanvas-title" id="offcanvasScrollingLabel">Profile</h1>
      <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
    </div>
    <div class="offcanvas-body">
      <p><strong>Name : </strong> <span><%= name %></span></p><br>
      <p><strong>Age : </strong><span><%= age %></span></p><br>
      <p><strong>Email : </strong><%= email %></p><br>
      <p><strong>Mobile No : </strong><span><%= phn %></span></p><br>
      <p><strong>Account Creation Date : </strong><span><%= createdAt %></span></p><br>



    </div>
  </div>