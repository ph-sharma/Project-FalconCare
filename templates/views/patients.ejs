    <style>
        main.css * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;

        }
        body{
            padding: 10px;
        }


        #navbar {
            background-color: black;
            width: 100%;
            opacity: 0.9;
            padding: 0px 10px 0px 50px;
            margin-bottom: 10px;
        }
        
        #footer-links{
            z-index: -5;
        }
        .container-1 {
            border: 2px solid black;
            border-radius: 12px;
            position: -webkit-sticky;
            position: sticky;
            top: 60px;
            flex-direction: row;
            display: flex;
            flex-wrap: wrap;
            justify-items: auto;
            margin: 20px;
            background-color: #16213E;
            opacity: 1;
        }

        .attribute {
            flex: 0.5em;
            margin: 10px;
            border: 2px solid black;
            border-radius: 12px;
            text-align: center;
            background-color: #1B1B2F;
            color: white;
        }

        .rules {
            text-align: center;
        }

        .att {
            border: 2px solid white;
            border-radius: 2px;
            padding: 2px;
            margin: 10px;
            background-color: #B9F8D3;
            
            opacity: 1;
        }

        .sugg {
            border: 2px solid white;
            border-radius: 2px;
            padding: 2px;
            background-color: #F47C7C;
            margin: 1px;
            opacity: 1;

        }

        li {
            display: inline-block;
        }

        .suggest {
            flex: 0.5em;
            margin: 10px;
            display: flexbox;
            border: 2px solid black;
            border-radius: 12px;
            text-align: start;
            background-color: #1B1B2F;
            color: white;
            overflow-y: scroll;
            height: 250px;
            opacity: 0.8;
        }

        .table {
            margin-right: 1050px;
            width: 80%;
            align-items: center;
            text-align: center;

        }

        

        tr>li {
            color: black;
        }

        ul {
            list-style-type: none;
        }

        body {
            font-family: 'Roboto', Helvetica, sans-serif;
        }

        .app {
            display: flex;
            flex-flow: column;

            width: 100vw;
            height: 100vh;
        }

        .lists {
            display: flex;
            flex: 1;
            width: 100%;
            overflow-x: scroll;
        }

        .btn {
            /* align-items: center; */
            margin: auto;
        }
        .navhead{
            color: white;
            font-size: 24px;
            font-weight: 200;
        }

        .lists .list {
            display: flex;
            flex-flow: column;
            flex: 1;

            width: 100%;
            min-width: 250px;
            max-width: 350px;
            height: 100%;
            min-height: 150px;

            background-color: rgba(0, 0, 0, 0.1);
            margin: 0 15px;
            padding: 8px;
            transition: all 0.2s linear;
        }

        .w {
            width: 10%;
        }

        .a {
            width: 20%;
        }

        

        

        li {
            color: black;
        }
    </style>
