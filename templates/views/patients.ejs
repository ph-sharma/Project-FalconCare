    <h1><b>Patient List</b></h1>
    <table class="table table-striped table-dark" >
        <thead>
          <tr>
        
            <th scope="col">Name</th>
            <th scope="col">Age</th>
            <th scope="col">Phone Number</th>
            <th scope="col">Email</th>
         
          </tr>
        </thead>
        <tbody>
            <% records.forEach(function(row){%>
                <tr>
                    
                    <td><%= row.name %></td>
                    <td><%= row.age %></td>
                    
                    <td><%= row.phn %></td>
                    <td><%= row.email %></td>
                    
                </tr>
            <%})%>
        </tbody>
      </table>



