  <script>
    const inpFile = document.getElementById("inpFile");
    const btnUpload = document.getElementById("btnUpload");
    const resultText = document.getElementById("resultText");

    btnUpload.addEventListener("click", () => {
      const formData = new FormData();

      formData.append("pdfFile", inpFile.files[0]);

      fetch("/extract-text", {
        method: "post",
        body: formData
      }).then(response => {
        return response.text();
      }).then(extractedText => {
        console.log(extractedText)
        resultText1.value = extractedText.trim().match(/[0-9]+\.+[0-9]+/g)[0];
        resultText2.value = extractedText.trim().match(/[0-9]+\.+[0-9]+/g)[1];
        resultText3.value = extractedText.trim().match(/[0-9]+\.+[0-9]+/g)[2];
        resultText4.value = extractedText.trim().match(/[0-9]+\.+[0-9]+/g)[4];
        resultText5.value = extractedText.trim().match(/[0-9]+\.+[0-9]+/g)[5];
        resultText6.value = extractedText.trim().match(/[0-9]+\.+[0-9]+/g)[6];
      });
    });
  </script>
  <script>
    function printDiv() {
