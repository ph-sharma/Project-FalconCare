                for (let i = 0; i < tbody.children.length; i++) {
                    const tr = tbody.children[i];
                    jsonData["SERIAL_NUMBER"].push(tr.children[0].innerText);
                    jsonData["ATTRIBUTES"].push(tr.children[1].innerText);
                    jsonData["MIN_VALUE"].push(tr.children[2].firstChild.value);

                    jsonData["MAX_VALUE"].push(tr.children[3].firstChild.value);
                    jsonData["SUGGESTIONS"].push(tr.children[4].innerText);
                }
                // document.getElementById("show-data").innerText = JSON.stringify(jsonData,undefined,4);
