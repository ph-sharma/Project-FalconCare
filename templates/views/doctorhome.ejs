        <script>
            function ondragoverHandler(event) {
                event.preventDefault();
            }

            function ondragstartHandler(event) {
                event.dataTransfer.setData("technology", event.target.id);
                event.dataTransfer.setData("dragid", event.target.getAttribute("dragid"));
            }

            function ondropHandler(event) {
                var data = event.dataTransfer.getData("technology");
                var dragid_src = event.dataTransfer.getData("dragid");

                if (dragid_src === "attr") {
                    if (event.target.getAttribute("dragid") === "attr-drop") {
                        if (event.target.children.length < 1)
                            event.target.appendChild(document.getElementById(data));
                    } else if (event.target.getAttribute("dragid") === "attr-bucket")
                        event.target.appendChild(document.getElementById(data));
                }
                else if (dragid_src == "sugg") {
                    if ((event.target.getAttribute("dragid") === "sugg-drop") || (event.target.getAttribute("dragid") === "sugg-bucket")) {
                        event.target.appendChild(document.getElementById(data));
                    }
                }

            }
        </script>
